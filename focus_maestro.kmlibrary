							<key>Text</key>
							<string>tell application "Keyboard Maestro Engine"	set togglApiKey to getvariable "FoMa_toggl_api_key"end tellactivate application "JSON Helper"tell application "JSON Helper"	set togglTimer to (fetch JSON from "https://www.toggl.com/api/v8/time_entries/current" name togglApiKey password "api_token")end tellset togglId to (|id| of |data| of togglTimer) as textset AppleScript's text item delimiters to {"E"}set multiplier to last text item of togglIdset togglId to first text item of togglIdset AppleScript's text item delimiters to {","}set togglId to (first text item of togglId &amp; last text item of togglId)set togglPid to (pid of |data| of togglTimer) as texttry	set togglDesc to  (description of |data| of togglTimer) as texton error	try		set togglDesc to (description of |data| of togglTimer) as text	on error		set togglDesc to ""	end tryend trytell application "Keyboard Maestro Engine"	setvariable "FoMa_toggl_timer_id" to togglId	setvariable "FoMa_toggl_timer_desc" to togglDesc	setvariable "FoMa_toggl_timer_pid" to togglPidend tell</string>
